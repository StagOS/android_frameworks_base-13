<resources>
    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Proximity check on screen on -->
    <java-symbol type="bool" name="config_proximityCheckOnWake" />

    <!-- Proximity check timeout -->
    <java-symbol type="integer" name="config_proximityCheckTimeout" />

    <!-- Proximity check on screen on default -->
    <java-symbol type="bool" name="config_proximityCheckOnWakeEnabledByDefault" />

    <!-- App error dialog -->
    <java-symbol type="id" name="aerr_copy" />
    <java-symbol type="string" name="url_copy_success" />
    <java-symbol type="string" name="url_copy_failed" />

    <!-- Whether device has physical tri state switch -->
    <java-symbol type="bool" name="config_hasAlertSlider" />

    <!-- The location of the devices physical tri state switch -->
    <java-symbol type="integer" name="config_alertSliderLocation" />

    <!-- Whether key handler sends intent when changing slider position -->
    <java-symbol type="string" name="config_alertSliderIntent" />

    <!-- Whether to force enable multi resolution configuration for camera -->
    <java-symbol type="bool" name="config_forceMultiResolution" />

    <!-- For dynamic default font styles -->
    <java-symbol type="string" name="config_bodyFontFamily" />

    <!-- Optimized charge notification -->
    <java-symbol type="string" name="optimized_charge_notification_channel_label" />
    <java-symbol type="string" name="optimized_charge_notification_title" />
    <java-symbol type="string" name="optimized_charge_notification_text" />

    <!-- Light / Regular style fonts -->
    <java-symbol type="string" name="config_lightFontFamily" />
    <java-symbol type="string" name="config_regularFontFamily" />

    <!-- Advanced reboot -->
    <java-symbol type="string" name="reboot_to_recovery_title" />
    <java-symbol type="string" name="reboot_to_recovery_message" />
    <java-symbol type="string" name="reboot_to_bootloader_title" />
    <java-symbol type="string" name="reboot_to_bootloader_message" />
    <java-symbol type="string" name="reboot_title" />
    <java-symbol type="string" name="reboot_message" />

    <!-- Global Actions pref list -->
    <java-symbol type="array" name="custom_config_globalActionsList" />

    <!-- Whether device has dash charging support -->
    <java-symbol type="bool" name="config_hasDashCharger" />

    <!-- Whether device has warp charging support -->
    <java-symbol type="bool" name="config_hasWarpCharger" />

    <!-- Whether device has VOOC charging support -->
    <java-symbol type="bool" name="config_hasVoocCharger" />

    <!-- Whether device has turbo power charging support -->
    <java-symbol type="bool" name="config_hasTurboPowerCharger" />

    <!-- Paths to OP warp charging status file -->
    <java-symbol type="string" name="config_opWarpChargerStatusPaths" />

    <!-- Path to fast charging status file to detect whether an oem fast charger is active -->
    <java-symbol type="array" name="config_oemFastChargerStatusPaths" />

    <!-- Wether to handle audio direction changes between major call states -->
    <java-symbol type="bool" name="config_handleAudioDirectionChangesBetweenCallStateChanges" />

    <!-- Enable face auth only when swiping security view -->
    <java-symbol type="bool" name="config_faceAuthOnlyOnSecurityView" />
</resources>
